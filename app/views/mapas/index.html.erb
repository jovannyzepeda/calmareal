<p id="notice"><%= notice %></p>

<h1>Listing Mapas</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Descripcion</th>
      <th>Lon</th>
      <th>Alt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mapas.each do |mapa| %>
      <tr>
        <td><%= mapa.titulo %></td>
        <td><%= mapa.descripcion %></td>
        <td><%= mapa.lon %></td>
        <td><%= mapa.alt %></td>
        <td><%= link_to 'Show', mapa %></td>
        <td><%= link_to 'Edit', edit_mapa_path(mapa) %></td>
        <td><%= link_to 'Destroy', mapa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mapa', new_mapa_path %>
